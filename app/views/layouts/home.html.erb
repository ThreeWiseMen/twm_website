            <li class="<%= sense_page("home", "contact") %>"><%= link_to "Contact Us", :controller => "home", :action => "contact" %></li>
            <li class="<%= sense_page("tools", "index") %>"><%= link_to "Customer Tools", :controller => "home", :controller => "tools", :action => "index" %></li>
